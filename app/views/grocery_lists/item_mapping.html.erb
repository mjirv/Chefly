<h2><strong>Your Grocery List</strong></h2>
<h4>Mapping</h4>
<%= button_to "Done", grocery_list_path(@grocery_list.id), :method => :get %>
<p>
	<% @grocery_list.grocery_list_items.each do |item| %>
		<% if item.name[-1] != ":" %>
			<%= item.to_s %> | <%= item.item_name.to_s %> | <%= link_to "Map", map_grocery_list_item_path(item.id), :method => :get %>
			<br />
		<% end %>
	<% end %>
</p>